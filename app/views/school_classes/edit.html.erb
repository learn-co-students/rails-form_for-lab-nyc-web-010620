<h1>School Class Update Form</h1>
<%= form_for(@school_class) do |f| %>
    <label>School Title:</label><br>
    <%= f.text_field :title %>
    <label>Room Number:</label><br>
    <%= f.number_field :room_number %>
    <%= f.submit "Update School class" %>
<% end %>